{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "datafactory-quigg"
		},
		"QuiggDataLakeDemo_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'QuiggDataLakeDemo'"
		},
		"AzureSynapseAnalytics_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'AzureSynapseAnalytics'"
		},
		"QuiggDataLakeDemo_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://datalakedemoquigg.dfs.core.windows.net/"
		},
		"AzureSynapseAnalytics_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "quiggdemo.sql.azuresynapse.net"
		},
		"AzureSynapseAnalytics_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "datapool"
		},
		"AzureSynapseAnalytics_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "sqladminuser"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "datafactory-quigg_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"QuiggDataLakeDemo_accountKey": {
						"value": "[parameters('QuiggDataLakeDemo_accountKey')]"
					},
					"AzureSynapseAnalytics_password": {
						"value": "[parameters('AzureSynapseAnalytics_password')]"
					},
					"QuiggDataLakeDemo_properties_typeProperties_url": {
						"value": "[parameters('QuiggDataLakeDemo_properties_typeProperties_url')]"
					},
					"AzureSynapseAnalytics_properties_typeProperties_server": {
						"value": "[parameters('AzureSynapseAnalytics_properties_typeProperties_server')]"
					},
					"AzureSynapseAnalytics_properties_typeProperties_database": {
						"value": "[parameters('AzureSynapseAnalytics_properties_typeProperties_database')]"
					},
					"AzureSynapseAnalytics_properties_typeProperties_userName": {
						"value": "[parameters('AzureSynapseAnalytics_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}